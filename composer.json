  "keywords": ["laravel", "eloquent", "database", "postgresql", "database"],
  "license": "MIT",
  "homepage": "https://Mammutgroup.com",
  "authors": [
    {
      "name": "Mirza Pasic",
      "email": "mirza@Mammutgroup.com"
    },
